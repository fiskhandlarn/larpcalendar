{
  "name": "larpcalendar",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "start": "firebase emulators:exec 'yarn nx serve' --import ./scripts/firebase/firestore-export --ui",
    "test": "firebase emulators:exec 'nx test' --import ./scripts/firebase/firestore-export",
    "e2e": "firebase emulators:exec 'nx e2e' --import ./scripts/firebase/firestore-export",
    "lint": "yarn nx lint",
    "build": "nx build",
    "affected:test": "firebase emulators:exec 'nx affected --target=test' --import ./scripts/firebase/firestore-export",
    "affected:e2e": "firebase emulators:exec 'nx affected --target=e2e' --import ./scripts/firebase/firestore-export",
    "affected:build": "nx affected --target=build",
    "affected:lint": "yarn nx affected --target=lint",
    "emulators:start": "firebase emulators:start --import ./scripts/firebase/firestore-export",
    "emulators:export": "firebase emulators:export ./scripts/firebase/firestore-export",
    "prepare": "husky install"
  },
  "private": true,
  "dependencies": {
    "@chakra-ui/icons": "^1.0.15",
    "@chakra-ui/react": "^1.6.7",
